{
  "_version": "1.37.0",
  "sap.app": {
    "id": "zpoapproval",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.4",
      "toolsId": "69d72e19-dbdd-4d1c-ba76-6b5fc0bcaa86"
    },
    "dataSources": {
      "ZVECV_PURCHASE_ORDER_APPROVAL_SRV": {
        "uri": "/sap/opu/odata/sap/ZVECV_PURCHASE_ORDER_APPROVAL_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.96.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zpoapproval.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
          "settings":{	"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"},
        "dataSource": "ZVECV_PURCHASE_ORDER_APPROVAL_SRV",
        "preload": true
       
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "zpoapproval.view",
        "controlAggregation": "pages",
        "controlId": "App",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "pattern": "",
          "name": "FirstPage",
          "target": [
            	"POApprovalMaster",
						"POApprovalDetail"
          ]
        },
        {
          "pattern": "FirstPage1",
          "name": "FirstPage1",
          "target":[	"POApprovalMaster"]
        },
        {
          "name": "POApprovalDetail",
          "pattern":  "POApprovalDetail/{PO_No}/POApprovalDetail1/{PO_Status}/POApprovalDetailsid/{Sid}/Dept/{Dept}/{Type}",
          "target":[	"POApprovalMaster",
          "POApprovalDetail"]

        },{
          "name": "POApprovalDetailChange",
          "pattern": "POApprovalDetail/{PO_No}/POApprovalDetailsid/{Sid}/Dept/{Dept}/{Type}",
          "target":[
            	"POApprovalMaster",
						"POApprovalDetail"
          ]
        },{
          "name": "POApprovalDetailForTab",
          "pattern": "POApprovalDetail/{PO_No}/POApprovalDetailsid/{Sid}/POApprovalDetailALP/{TemPStatusforAlApp}/{Type}",
          "target":[
            	"POApprovalMaster",
						"POApprovalDetail"
          ]
        },{
          "name": "POApprovalDetailForDevAppr",
          "pattern": "POApprovalDetail/{PO_No}/POApprovalDetailsid/{Sid}/POApprovalDetailDevA/{TemPStatusforAlApp",
          "target":[	"POApprovalMaster",
          "POApprovalDetail"]
        },{
          "name": "poapprovaldetail",
          "pattern": "poapprovaldetail/{status}",
          "target":[	"POApprovalMaster",
          "POApprovalDetail"]
        },{
          "name": "POApprovalMaster",
          "pattern": "POApprovalMaster",
          "target":[	"POApprovalMaster",
          "POApprovalDetail"]
        },{
          "name": "POApprovalMaster",
          "pattern": "POApprovalMaster/{TempStatus}",
          "target":[
            	"POApprovalMaster",
						"POApprovalDetail"
          ]
        },{
          "name": "RouteSApp",
          "pattern": "RouteSApp",
          "target":[	"POApprovalMaster",
          "POApprovalDetail"]
        }
      ],
      "targets": {
        "TargetSApp": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "App",
          "viewName": "SApp"
        },
        "POApprovalMaster":{
          "viewName": "POApprovalMaster",
          "controlAggregation": "masterPages"
          	
        },
        "POApprovalDetail":{
          "viewName": "POApprovalDetail",
           "controlAggregation": "detailPages"
          
        }
      }
    },
    	"_version": "1.2.0",
    "rootView": {
      "viewName": "zpoapproval.view.SApp",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "sap.platform.hcp": {
      "uri": "webapp",
      "_version": "1.1.0"
    },
    "sap.platform.abap": {
      "uri": "/sap/bc/ui5_ui5/sap/zpoapproval",
      "_version": "1.1.0"
    }
  }
 
}